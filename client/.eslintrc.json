{
  "extends": ["next/core-web-vitals", "prettier"],
   "parserOptions": {
       "project": "./tsconfig.json"
    },
    "rules": {
      "no-unused-vars": ["error", {
          "vars": "all",
          "args": "after-used",
          "caughtErrors": "all",
          "ignoreRestSiblings": false,
          "reportUsedIgnorePattern": false
      }]
  }
}
